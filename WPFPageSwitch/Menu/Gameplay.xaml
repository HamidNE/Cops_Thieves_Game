<UserControl x:Class="WPFPageSwitch.Menu.GamePlay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFPageSwitch.Menu"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="750">
    <Grid Background="White">
        <Label x:Name="label" Content="" HorizontalAlignment="Left" Height="60" Margin="128,53,0,0" VerticalAlignment="Top" Width="345"/>
        <TextBox x:Name="xTextBox" HorizontalAlignment="Left" Height="56" Margin="244,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="92"/>
        <TextBox x:Name="yTextBox" HorizontalAlignment="Left" Height="56" Margin="400,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86"/>
        <Button x:Name="button" Content="Set" HorizontalAlignment="Left" Height="30" Margin="327,270,0,0" VerticalAlignment="Top" Width="97" Click="button_Click"/>
        <Label x:Name="label1" Content="X" HorizontalAlignment="Left" Margin="183,192,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1_Copy" Content="Y" HorizontalAlignment="Left" Margin="363,192,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
